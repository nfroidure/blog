{
  "name": "insertafter",
  "version": "0.1.0",
  "description": "Nicolas Froidure's homepage",
  "metapak": {
    "data": {
      "files": "src/**/*.js"
    },
    "configs": [
      "www",
      "main"
    ],
    "ignore": [
      "www"
    ]
  },
  "scripts": {
    "build": "gulp --prod",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cz": "env NODE_ENV=${NODE_ENV:-cli} git cz",
    "dev": "gulp",
    "lint": "eslint src/**/*.js",
    "metapak": "metapak",
    "postinstall": "npm run metapak",
    "precz": "npm t && npm run lint && npm run metapak -- -s",
    "prepublish": "gulp --prod",
    "preversion": "npm t && npm run lint && npm run metapak -- -s",
    "publish": "gulp publish --prod",
    "test": "gulp build --prod --noreq --nolr --nobro --nowatch --nosrv",
    "test:urls": "reachable-urls contents/**/*.md contents/**/*.html",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "homepage": "http://insertafter.com",
  "repository": {
    "type": "git",
    "url": "git://github.com/nfroidure/blog"
  },
  "bugs": {
    "url": "https://github.com/nfroidure/blog/issues"
  },
  "author": {
    "name": "Nicolas Froidure",
    "email": "nicolas.froidure@insertafter.com",
    "url": "http://insertafter.com/en/index.html"
  },
  "devDependencies": {
    "asttpl": "^1.0.0",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "browserify": "^11.1.0",
    "buildbranch": "^2.0.0",
    "combine-stream": "0.0.4",
    "commitizen": "^3.0.4",
    "conventional-changelog-cli": "^2.0.5",
    "coveralls": "2.11.15",
    "css-queryless": "^0.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "3.4.0",
    "eslint-config-simplifield": "4.1.1",
    "express": "^4.13.3",
    "favicons": "^3.1.3",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-clone": "^1.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-cond": "^1.0.0",
    "gulp-iconfont": "^5.0.0",
    "gulp-imagemin": "^2.3.0",
    "gulp-jshint": "^1.11.2",
    "gulp-less": "^3.0.3",
    "gulp-livereload": "^3.8.0",
    "gulp-load-plugins": "^1.0.0-rc",
    "gulp-marked": "^1.0.0",
    "gulp-mdvars": "^2.0.0",
    "gulp-minify-css": "^1.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-rework": "^1.0.3",
    "gulp-spawn": "^0.3.0",
    "gulp-streamify": "^1.0.2",
    "gulp-svgmin": "^1.2.0",
    "gulp-util": "^3.0.6",
    "gulp-vartree": "^2.0.1",
    "gulp-watch": "^4.3.5",
    "html-to-text": "^3.1.0",
    "internal-ip": "^1.0.1",
    "istanbul": "0.4.5",
    "lunr": "^0.7.2",
    "metapak": "2.0.0",
    "metapak-nfroidure": "8.0.0",
    "mocha": "3.2.0",
    "mocha-lcov-reporter": "1.2.0",
    "nunjucks": "^2.0.0",
    "open": "^0.0.5",
    "plexer": "^1.0.1",
    "reachable-urls": "^0.9.0",
    "request": "^2.62.0",
    "rework-rem2px": "^0.0.1",
    "rimraf": "~2.4.3",
    "streamfilter": "^1.0.5",
    "tiny-lr": "^0.1.6",
    "varstream": "^0.3.2",
    "yargs": "^3.25.0"
  },
  "dependencies": {
    "debug": "1.0.0",
    "moment": "^2.10.6"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.9.5"
  },
  "contributors": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "greenkeeper": {
    "ignore": [
      "commitizen",
      "cz-conventional-changelog",
      "conventional-changelog-cli"
    ]
  }
}
